<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageButton
        android:id="@+id/imgbtn1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="63dp"
        android:onClick="tirar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/vaso"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imgv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.335"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgbtn1"
        app:srcCompat="@drawable/d1" />

    <ImageView
        android:id="@+id/imgv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toEndOf="@+id/imgbtn1"
        app:layout_constraintTop_toBottomOf="@+id/imgbtn1"
        app:srcCompat="@drawable/d1" />

    <ImageView
        android:id="@+id/imgv5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="@+id/imgv4"
        app:layout_constraintTop_toBottomOf="@+id/imgv4"
        app:srcCompat="@drawable/d1" />

    <ImageView
        android:id="@+id/imgv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toEndOf="@+id/imgv1"
        app:layout_constraintTop_toBottomOf="@+id/imgv1"
        app:srcCompat="@drawable/d1" />

    <ImageView
        android:id="@+id/imgv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="36dp"
        app:layout_constraintStart_toStartOf="@+id/imgv2"
        app:layout_constraintTop_toBottomOf="@+id/imgv2"
        app:srcCompat="@drawable/d1" />

    <ImageView
        android:id="@+id/imgv6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginTop="36dp"
        app:layout_constraintStart_toStartOf="@+id/imgv5"
        app:layout_constraintTop_toBottomOf="@+id/imgv5"
        app:srcCompat="@drawable/d1" />

</androidx.constraintlayout.widget.ConstraintLayout>